<tr>
  <td class="date"><%= link_to registro.data, edit_registro_path(registro), data: { behavior: 'load-modal'} %></td>
  <% if registro.transferencia? %>
  <td class="text-very-muted">
    <%= icn(:exchange_alt, registro.descricao) %>
  <% else %>
  <td class="text-muted">
    <%= registro.descricao %>
  <% end %>
  </td>
  <td class='text-right <%= registro.cd %>' ><%= curr(registro.valor_cd) %></td>
</tr>
